meta {
  name: reset_password
  type: http
  seq: 5
}

post {
  url: {{base_url}}/api/auth/reset-password
  body: json
  auth: none
}

headers {
  Content-Type: application/json
}

body:json {
  {
    "token": "{{reset_token}}",
    "password": "{{password}}"
  }
}

tests {
  test("status is 200", () => {
    expect(res.status).to.equal(200);
  });
  
  test("returns message", () => {
    expect(res.body.message).to.be.a("string");
  });
}
